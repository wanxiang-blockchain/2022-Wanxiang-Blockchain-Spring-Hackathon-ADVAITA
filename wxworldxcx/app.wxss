/* 弹窗 */
.m-mask {
	background-color: rgba(0, 0, 0, 0.3);
	position: fixed;
	left: 0;
	right: 0;
	top: 0;
	bottom: 0;
	margin: auto;
	z-index: 999;
}

.mask-box {
	width: 666rpx;
	height: 395rpx;
	background: rgba(255, 255, 255, 1);
	box-shadow: 0px 2rpx 10rpx 0px rgba(0, 0, 0, 0.25);
	border-radius: 15rpx;
	text-align: center;
	position: absolute;
	left: 0;
	right: 0;
	top: 0;
	bottom: 0;
	margin: auto;

}

.mask-title {
	font-size: 36rpx;
	font-weight: bold;
	color: rgba(3, 19, 49, 1);
	margin-top: 56rpx;
	margin-bottom: 31rpx;
}

.mask-content {
	height: 120rpx;
	font-size: 28rpx;
	text-align: center;
}

.mask-group {
	width: 100%;
	box-sizing: border-box;
	padding: 0 40rpx;
	display: flex;
	flex-direction: row;
	flex-wrap: nowrap;
	justify-content: space-between;
	position: absolute;
	bottom: 50rpx;
}

.mask-btn {
	width: 270rpx;
	height: 80rpx;
	line-height: 80rpx;
	font-size: 32rpx;
	text-align: center;
