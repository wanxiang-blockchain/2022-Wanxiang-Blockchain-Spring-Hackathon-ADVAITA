/*弹窗样式s*/
.openGoodsList {
    position:fixed;
    height:100px;
    width:50px;
    background:#B7DCD2;
    border-top-right-radius: 8%;
    border-bottom-right-radius: 8%;
    /*border-radius:50%;*/
    top:18%;
    left:0;
    z-index:9999;
}

.openGoodsList image {
  width: 25px;
  height: 25px;
  position: absolute;
  top: 12px;
  left: 2px;
}

.goodsListPage {
  position: fixed;
  width: 100%;
  height: 100%;
  z-index: 9999;
  /*background: rgba(0, 0, 0, 0.5);*/
  top: 0;
  transition: 0.3s;
}

.right_100 {
  right: -100%;
}

.right_0 {
  right: 0;
}

.left_100 {
  left: -100%;
}

.left_0 {
  left: 0;
}

.goodsListPage .list-right {
  background: #fff;
  width: 22%;
  position: absolute;
  left: 0;
  top: 0;
  height: 100%;
  border-right: 2px solid #9bcbba;
}

.goodsListPage .list-right .closeGoodList {
    position:absolute;
    height:100px;
    width:50px;
    background:#B7DCD2;
    border-top-right-radius: 8%;
    border-bottom-right-radius: 8%;
    /*border-radius:50%;*/
    top:18%;
    right:-50px;
}

.goodsListPage .closeGoodList image {
  width: 25px;
  height: 25px;
  position: absolute;
  top: 13px;
  left: 6px;
}

.goodsListPage .list-right .operate {
  position: absolute;
  bottom: 0;
  height: 50px;
  border-top: 1px #d5d5d5 solid;
  width: 100%;
  left: 0;
  overflow: hidden;
}

.goodsListPage .list-right .operate>view {
  float: left;
  width: 50%;
  height: 100%;
}

.goodsListPage .list-right .operate .cart {
  background: #fff;
  text-align: center;
}

.operate .cart>view {
  overflow: hidden;
  margin: 0 auto;
  width: 132px;
}

.operate .cart .icon {
  width: 20px;
  height: 20px;
  float: left;
  margin-top: 15px;
}

.operate .cart text {
  height: 20px;
  line-height: 20px;
  margin-left: 5px;
  margin-top: 15px;
  display: inline-block;
  max-width: 102px;
  overflow:hidden;
}

.goodsListPage .list-right .operate .sure {
  background: #fe6263;
  text-align: center;
  line-height: 50px;
  color: #fff;
}

.goodsListPage .list-right .scrollList {
  padding: 00 50px0;
  height: 100%;
  box-sizing: border-box;
}

.list-right .scrollList .item-list {
  border-bottom: 1px #d5d5d5 solid;
  overflow: hidden;
  padding:10px 0 10px 0;
}

.list-right .scrollList .item-list .left {
  float: left;
  width: 64px;
  height: 64px;
}

.list-right .scrollList .item-list .left image {
  width: 100%;
  height: 100%;
}

.list-right .scrollList .item-list .center {
  margin: 0 72px;
}

.item-list .center text {
  display: block;
  height: 22px;
  line-height: 22px;
  font-size: 12px;
  color: #b6b6b6;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.item-list .center text.goodsName {
  font-size: 14px;
  color: #333;
}

.list-right .scrollList .item-list .right {
  float: right;
  width: 65px;
  overflow: hidden;
  margin-top: 23px;
}

.item-list .right view {
  float: left;
}

.item-list .right .count-option, .item-list .right .count-num {
  font-size: 12px;
  width: 20px;
  height: 20px;
  line-height: 20px;
  text-align: center;
}

.item-list .right .count-num {
  width: 24px;
}

.item-list .right .plus {
  background-image: url('../../img/plus-icon.png');
  background-size: 100% 100%;
}

.item-list .right .add {
  background-image: url('../../img/add-icon.png');
  background-size: 100% 100%;
}

.list-right .scrollWrap {
  height: 180px;
  position: absolute;
  bottom: 50px;
  left: 0;
  width: 100%;
}

.list-right .scrollDetail {
  position: absolute;
  top: 15px;
  left: 0;
  width: 100%;
  height: 165px;
  background: #f6f6f6;
  z-index: 101;
  padding: 8px 0 0 0;
  box-sizing: border-box;
}

.scrollWrap .closeDetail {
  background: #f6f6f6;
  border-radius: 50%;
  width: 15px;
  height: 15px;
  position: absolute;
  top:0;
  right: 12px;
  color: #797979;
  text-align: center;
  line-height: 15px;
  font-size: 27px;
  z-index: 102;
}

.detailCon .detailList {
  height: 34px;
  line-height: 34px;
  color: #b6b6b6;
  font-size: 14px;
  border-bottom: #dedede solid 1rpx;
}

.detailCon .detailList text {
  display: inline-block;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.detailCon .detailList .name {
  color: #333;
  font-size: 15px;
  width: 55%;
  margin-right: 15px;
}
.allProduct{
    float: left;
    width:18px;
    line-height:18px;
    padding-left: 12px;
    padding-top: 13px;
    font-size:16px;
    color: #fff;
    font-weight: 100;
}
.title-text{
  margin-top: 10px;
  float: left;
  width: 25%;
}
.title{
  display: block;
  color: #90caba;
  text-align: center;
}
.title-a{
  display: block;
  color: #333;
  text-align: center;
  font-size: 15px;
  line-height: 30px;
}
.line{
  border-bottom: 1px solid #ccc;
  margin-top: 30px;
}
