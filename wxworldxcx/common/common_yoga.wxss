page{
    background: #FBFBFD;
    color: #3E3A39;
}
.topname{
    width: 72%;
    float:left; 
    margin-top: 14px;
    height: 26px;
    line-height: 26px;
    padding-right: 11px;
    color:#fff; 
    text-align: right;
    font-size:16px;
    position:absolute;
    text-shadow:1px 2px 1px #000;  
}
.leftIcon{
    width:1.5rem;
    height:1.5rem;
    padding:0.5rem;
}
.Head{
    text-align:center;
    padding-bottom:3.5rem;
}
.Head image{
    width:3.5rem;
    height:3.5rem;
    border-radius:2rem;
    margin-top:2rem;
    /*margin-left:2.5rem;*/
}
.Head text{
    display:block;
    width:100%;
    float:left;
    line-height:22px;
    font-size:16px;
    font-weight: 600;
}
.listAdd{
    padding-left:1.8rem;
    line-height:3rem;
}

.listRightAdd .rightIcon{
    width:1rem ;
    height: 1rem;
    padding-left:5.8rem;
    padding-top:0.1rem;
}
text{
    color:#ffffff;
}
.content {
    color: #ffffff;
    font-size: 40px;
}
.bg {
    background: url('https://wx.qlogo.cn/mmopen/vi_32/DYAIOgq83eqB5ibWEBNhv6Eib3y7pAy4ddwicusxbB4jrrCmBF5FlBVOt2bXj1S42Xc15Vsrqpo7BT0L1XKnwPjyw/0') no-repeat;
    width:100%;
    height:100%;
    text-align: center;
    line-height: 100%;
}
.bg-blur {
    float: left;
    width: 100%;
    background-repeat: no-repeat;
    background-position: center;
    background-size: cover;
    /*-webkit-filter: blur(15px);
    -moz-filter: blur(15px);
    -o-filter: blur(15px);
    -ms-filter: blur(15px);
    filter: blur(15px);*/
    -webkit-filter: brightness(.25);
}
.content-front {
  position:absolute;
  width:100%;
}
.fontWeightSet{
    font-weight: 600;
}
