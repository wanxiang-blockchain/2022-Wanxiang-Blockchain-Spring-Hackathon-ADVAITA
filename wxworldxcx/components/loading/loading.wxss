@font-face {
	font-family: "iconfont-load";
	src: url('//at.alicdn.com/t/font_2090808_7a8ro0k0vta.eot?t=1600853747762');
	/* IE9 */
	src: url('//at.alicdn.com/t/font_2090808_7a8ro0k0vta.eot?t=1600853747762#iefix') format('embedded-opentype'),
		/* IE6-IE8 */
		url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
		url('//at.alicdn.com/t/font_2090808_7a8ro0k0vta.woff?t=1600853747762') format('woff'),
		url('//at.alicdn.com/t/font_2090808_7a8ro0k0vta.ttf?t=1600853747762') format('truetype'),
		/* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
		url('//at.alicdn.com/t/font_2090808_7a8ro0k0vta.svg?t=1600853747762#iconfont') format('svg');
	/* iOS 4.1- */
}

.iconfont-load {
	font-family: "iconfont-load" !important;
	font-size: 16px;
	font-style: normal;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.icon-loading:before {
	content: "\e6a6";
}


/*纯白色圈*/
.loader {
	display: inline-block;
	font-size: 32rpx;
	animation: load-effect 1s infinite linear;
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	margin: auto;
}

/*动画：旋转360°*/
@keyframes load-effect {
	0% {
		-webkit-transform: rotate(0deg);
		transform: rotate(0deg);
	}

	100% {
		-webkit-transform: rotate(360deg);
		transform: rotate(360deg);
	}
}
