/*1~9字体*/
@font-face {
    font-family: 'DINPro';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: bold;
    font-style: normal;
}
/* 趋势 */
.enter-box{
	width: 686rpx;
	height: 130rpx;
	line-height: 130rpx;
	color: #fff;
	background: #E7980B;
	border-radius: 26rpx;
	margin: 32rpx auto 32rpx;
	padding-left: 74rpx;
	position: relative;
}
.enter-icon{
	height: 36rpx;
	line-height: 36rpx;
	position: absolute;
	left: 32rpx;
	top: 0;
	bottom: 0;
	margin: auto;
}

.enter-text{
	opacity: 0.68;
	font-size: 26rpx;
	position: absolute;
	right:28rpx;
	top: 0;
	bottom: 0;
	margin: auto;
}
.enter-back{
	font-size: 24rpx;
}

.flex-box {
	display: flex;
	flex-direction: row;
	flex-wrap: nowrap;
	justify-content: space-between;
}
.p-title{
  font-size: 36rpx;
  font-weight: bold;
  color: #333;
  text-align: left;
  margin: 30rpx 32rpx 32rpx;
}
/* list */
.log-item{
	width: 686rpx;
	background: #fff;
	border-radius: 10rpx;
	margin:32rpx auto;
	padding:32rpx 0 0 0;
	box-shadow: 0 0 20rpx 0 rgba(152,152,152,0.15);
	box-sizing: border-box;
}
.log-item:first-child{
	margin-top: 0;
}
.user-box{
	height: 100rpx;
	position: relative;
}
.log-img{
	width: 100rpx;
	height: 100rpx;
	border-radius: 50%;
	background-color: #ccc;
}
.log-score{
	font-size: 28rpx;
	font-family: PingFang-SC-Bold, PingFang-SC;
	font-weight: bold;
	color: #989898;
	position: absolute;
	left: 112rpx;
	top: -8rpx;
}
.log-num{
	font-size: 60rpx;
	font-family: DINPro-Bold, DINPro;
	font-weight: bold;
	color: #1966CA;
	line-height: 77rpx;
}
.log-name{
	font-size: 26rpx;
	font-family: PingFang-SC-Bold, PingFang-SC;
	font-weight: bold;
	color: #0D1345;
	line-height: 37rpx;
	position: absolute;
	left: 112rpx;
	bottom: -2rpx;
}
.log-text{
	font-size: 24rpx;
	color: #989898;
	line-height:30rpx;
}
.border-line{
	width: 100%;
	height: 2rpx;
	background-color: #e6e6e6;
	margin: 32rpx 0 0;
}
.more-arr{
	padding: 20rpx 0;
	text-align: center;
	font-size: 28rpx;
	color: #999;
	transition: all 0.3s;
}
.arr-rotate{
	transform: rotateZ(180deg);
}
/* 任务中心 */
.rask-more{
	/* height: 328rpx; */
	transition: all 0.3s;
}
.more-hide{
	height: 0;
	overflow: hidden;
}
.rask-box{
	margin: 32rpx 0;
}
.rask-item{
	width: 20%;
	text-align: center;
}
.rask-icon{
	color: #2B86FF;
	height: 90rpx;
	line-height: 104rpx;
	font-size: 64rpx;
}
.rask-text{
	font-size: 26rpx;
	font-weight: bold;
	color: #999;
	height: 37rpx;
	line-height: 37rpx;
}
.rask-title{
	font-size: 24rpx;
	color: #999;
	margin-top: 14rpx;
}
.rask-bold{
	font-weight: bold;
	color: #333;
}
.reason-delete {
	width: 124rpx;
	height: 48rpx;
	line-height: 48rpx;
	text-align: center;
	font-size: 20rpx;
	color: #fff;
	background-color: #E02020;
	border-radius: 24rpx;
	position: absolute;
	right: 190rpx;
	bottom: 32rpx;
}