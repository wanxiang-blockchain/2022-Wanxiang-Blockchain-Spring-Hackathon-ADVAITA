/*1~9字体*/
@font-face {
    font-family: 'DINPro';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: bold;
    font-style: normal;
}
page{
  background: #FAFAFA;
  overflow-y: hidden;
  overflow-x: hidden;
}
.btn{
	text-align: center;
}
/* TAB */
.data {
	height: 100rpx;
	line-height: 100rpx;
	color: rgba(255,255,255,0.6);
	background-color: #031331;
}

.data-box {
	width: 25%;
	font-size: 28rpx;
	text-align: center;
	position: relative;
}

.data .active {
	color: #fff;
}
.data .active::after{
	content: '';
	width: 40rpx;
	height: 5rpx;
	background: #FFFFFF;
	position: absolute;
	left: 0;
	right: 0;
	bottom: 14rpx;
	margin: auto;
}
.imbibition{
	width: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 999;
}
.block-box-tab{
	height: 100rpx;
}
/* TAB end */

/* 分数 */
.score-list{
	width: 664rpx;
	height: 307rpx;
	margin: 36rpx auto 30rpx;
	position: relative;
}
.score-bg{
	width: 664rpx;
	height: 307rpx;
	position: absolute;
	left: 0;
	top: 0;
	z-index: 0;
}
.score-item{
	width: 96rpx;
	height: 103rpx;
	text-align: center;
}
.score-text{
	font-size: 24rpx;
	color: rgba(255,255,255,0.7);
	margin-bottom: 18rpx;
}
.score-num{
	font-size: 36rpx;
	color: rgba(255,255,255,1);
}
.salt-list{
	width: 664rpx;
	height: 85rpx;
	margin: 0 auto 0;
	position: relative;
	border-radius: 10rpx;
	background: linear-gradient(180deg, #FFBF31 0%, #FF8B16 100%);
}
.salt-bg{
	width: 664rpx;
	height: 85rpx;
	position: absolute;
	left: 0;
	top: 0;
	z-index: 0;
}
.salt-tips{
	height: 85rpx;
	line-height: 85rpx;
	padding-left: 18rpx;
	font-size: 26rpx;
	color: #fff;
	position: relative;
	z-index: 1;
}
.salt-title{
	display: inline-block;
}

.block-box{
  height: 64rpx;
  width: 100%;
}
.no-data{
	height: 1rpx;
}
.user-top{
	/* height: 348rpx; */
  background: #031331;
  padding-top: 58rpx;
  padding-bottom: 30rpx;
}
.user-top .left{
  width: 72%;
  text-align: left;
  position: absolute;
  left: 0;
  top: 58rpx;
}
.user-top .left .avatar_url{
  width:128rpx;
  height: 128rpx;
  border-radius: 10em;
  margin: 10rpx 20rpx 0 60rpx;
  float: left;
  /*vertical-align: middle;*/
}
.user-top .left .btn{
  width: 120rpx;
  height: 40rpx;
  font-size: 24rpx;
  color: #fff;
  background:#1966CA;
  border-radius: 20rpx;
  padding: 0;
  margin-top:10rpx;
  float: left;
  line-height: 40rpx;

  /*vertical-align: middle;*/
}
.user-top .left .orange-btn{
	background: #FF9E20;
}
.user-top .left .nickname{
  color: #fff;
  font-size: 32rpx;
  margin-top: 6rpx;
  white-space:nowrap; 
  text-overflow:ellipsis; 
  overflow:hidden;
}
.user-top .left .starttime_text{
  color: #fff;
  font-size: 24rpx;
}
.user-top .right{
  margin-right: 60rpx;
}
.user-top .right .count{
  color: #fff;
  font-family: DINPro;
  font-size: 80rpx;
  text-align: center;
  margin-top: -16rpx;

}
.user-top .right .count .text{
  color: #fff;
  font-size: 28rpx;
  margin-left: 10rpx;
}
.user-top .right .title{
  color: #fff;
  font-size: 28rpx;
  font-weight: 400;
  margin-top: -25rpx;
}
.user-top .user-top-bottom{
  display: flex;
  align-items: center;
  justify-content: center;
}
.user-top .user-top-bottom .item{
  color: #fff;
  margin:38rpx 38rpx 0 38rpx;
}
.tags{
  width: 100%;
  
  height: 160rpx;
  position: fixed;
  left: 0;
  z-index: 10;
}
.tags .item{
  float: left;
  width: 25%;
  color: rgba(255,255,255,0.6);
  text-align: center;
  background: #031331;
  font-size: 28rpx;
  padding-top: 20rpx;

}
.tags .item.active{
  color: rgba(255,255,255,1);
  font-weight: bold;
}
.tags .item view{
  width: 16%;
  border-bottom: 5rpx solid rgba(0,0,0,0);
  margin: 15rpx auto;
}
.tags .item.active view{
  border-bottom: 5rpx solid #fff;
}
.tags-false{
  width: 100%;
  height: 160rpx;
  background: #031331;
}
.body-min .content{
  background: #FFF;
  width: calc(100% - 60rpx);
  margin:30rpx;
  box-shadow: 0 10rpx 30rpx 0 rgba(152,152,152,0.15);
  border-radius: 10rpx;
  padding: 40rpx 30rpx;
  
}
.body-min .content .type-title{
  width:690rpx;
  height:104rpx;
  line-height: 104rpx;
  text-align: center;
  color: #fff;
  background:rgba(25,102,202,1);
  box-shadow:0px 2rpx 4rpx 0px rgba(0,0,0,0.25);
  border-radius:10rpx 10rpx 52rpx 52rpx;
  position: relative;
  left: -30rpx;
  top: -40rpx;
}

.body-min .content .user{
  width: 100%;
  margin-bottom: 30rpx;
}
.body-min .content .user .left{
  width: 50%;
  text-align: left;
}
.body-min .content .user .left .avatar_url{
  width: 88rpx;
  height: 88rpx;
  border-radius: 10em;
  margin-right: 20rpx;
  vertical-align: middle;
}
.body-min .content .user .left .nickname{
  font-size: 32rpx;
  color: #282828;
}

.body-min .content .count{
  width: 100%;
  text-align: center;
  font-size: 100rpx;
  color: #1966CA;
  font-family: 'DINPro';
  height: 140rpx;
  line-height: 140rpx;
}

.body-min .content .count text{
  opacity: 0.8;
  font-size: 28rpx;
  color: #ADADAD;
  text-align: center;
  margin-left: 10rpx;
}
.body-min .content .grade{
  width: 100%;
  text-align: center;
  font-size: 32rpx;
  color: #ADADAD;
  margin-bottom: 20rpx;
  margin-top: -10rpx;
  height: 100rpx;
  line-height: 100rpx;
}


.picture-img{
  width: 100%;
}
.picture-text{
  text-align: center;
  font-size: 28rpx;
}

.p-title{
  font-size: 30rpx;
  /* border-left:6rpx solid #031331; */
  margin-left: 30rpx;
  color: #031331;
  font-weight: 500;
  text-align: left;
  margin-bottom: 20rpx;
  margin-top: 40rpx;
}
.p-title .line{
  height: 28rpx;
  width: 6rpx;
  background: #031331;
  margin:0;
  border: none;
  margin-right: 10rpx;
  display: inline-block;
  vertical-align: middle;
  margin-top: -4rpx;

}
.iconwenhaolveguo{
  font-size: 38rpx;
  color: #1966CA;
  display: inline-block;
  vertical-align: middle;
  margin-top: -5rpx;
  font-weight: 400;
}
.p-title .iconwenhaolveguo{
  font-size: 42rpx;
  color: #1966CA;
  display: inline-block;
  vertical-align: middle;
  margin-top: -4rpx;
  margin-left:5rpx;
}
.body-min .content .p-title{
  margin-top: 0;
}

.body-min{
  /*z-index: 9;*/
  margin-top: -70rpx;
  /*position: relative;*/
}
.wrapper {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
}

.help-content {
  width: 80%;
  border-radius: 10rpx;
  /*height: 120px;*/
  background-color: #fff;
  padding: 30rpx;
}

.help-content .title{
  font-size: 32rpx;
  color: #031331;
  text-align: center;
  font-weight: 500;
  margin-top: 10rpx;
  margin-bottom: 20rpx;
}
.help-content .item{
  font-size: 26rpx;
  color: #031331;
  line-height: 40rpx;
  margin-bottom: 20rpx;

}
.share-treport{
  position: fixed;
  width: 176rpx;
  height: 176rpx;
  right: 20rpx;
  bottom: 98rpx;
  border-radius: 10em;
  background: none;
}
.share-treport image{
  width: 100%;

}
.detail-btn{
	width: 100rpx;
	height: 40rpx;
	line-height: 40rpx;
	text-align: center;
	font-size: 24rpx;
	color: #fff;
	background: #1966CA;
	border-radius: 20rpx;
	display: inline-block;
	margin-left: 18rpx;
	vertical-align: middle;
}
.detail-back{
	font-size: 20rpx;
	margin-left: 6rpx;
	vertical-align: middle;
}