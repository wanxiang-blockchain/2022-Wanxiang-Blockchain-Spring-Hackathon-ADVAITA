
.count-div{
  background: rgba(255,255,255, 0.11);
  box-shadow: 4rpx 4rpx 7rpx 0rpx rgba(0,0,0,0.5);
  width: 90%;
  margin-left: 5%;
  border-radius: 14rpx;
  padding-bottom: 27rpx;
}
.count-div .top{
  margin: 0 31rpx;
  padding-top: 31rpx;
  padding-bottom: 24rpx;
  display: flex;
  justify-content: space-between;
  border-bottom: 1px solid rgba(65, 78, 106, 1);
}
.count-div .top .top-left{
  display: flex;
  
}
.count-div .top .top-left image{
  width: 72rpx;
  height: 72rpx;
  border-radius: 10em;

  
}
.count-div .top .top-left .info{
  color: #fff;
  margin-left: 22rpx;
}
.count-div .top .top-left .info .name{
  line-height: 1;
  font-weight: 400;
  font-size: 33rpx;
  max-width: 325rpx;
  white-space:nowrap;
  text-overflow:ellipsis; 
  overflow:hidden;
}
.count-div .top .top-left .info .time{
  line-height: 1;
  margin-top: 10rpx;
  font-size: 22rpx;
}

.count-div .top .top-right{
  font-size: 22rpx;
  color: #fff;
  
}

.count-div .top2{
  color: #fff;
  display: flex;
  justify-content:space-between;
  border-bottom: 1px solid rgba(65, 78, 106, 1);
  margin: 0 31rpx;
  padding: 12rpx 0;
}

.count-div .top2 .t{
  font-size: 25rpx;
  line-height: 76rpx;
}

.count-div .top2 .t text{
  color: rgba(153, 153, 153, 1);
}
.count-div .top2 .c{

  font-size: 65rpx;
  line-height: 70rpx;
}
.count-div .top2 .c text{
  margin-left: 10rpx;
  font-size: 22rpx;
  line-height: 1;
}
.count-div .top3{
  margin: 36rpx 31rpx 0 31rpx;
  padding: 12rpx 0;
  border-bottom: 1px solid rgba(65, 78, 106, 1);
}

.count-div .top3 .item{
  background: rgba(255,255,255,0.09);

  border-radius: 0 14rpx 14rpx 0;
  margin-bottom: 24rpx;
  margin-left: 109rpx;
  display: flex;
  padding: 14rpx 0;
  width: calc(100% - 109rpx);
  justify-content: space-around;
  position: relative;
}
.count-div .top3 .item .l-t{
  width: 109rpx;
  text-align: center;
  position: absolute;
  left: -109rpx;
  top:0;
  padding: 14rpx 0;
  border-radius: 14rpx 0 0 14rpx;

}
.red{
  color: rgba(255, 52, 52, 1);
}
.yellow{
  color: rgba(255, 179, 50, 1);
}
.blue{
  color: rgba(76, 201, 255, 1);
}
.green{
  color: rgba(138, 235, 127, 1);
}
.count-div .top3 .item .l-t .c,.count-div .top3 .item .d .c{
  line-height: 60rpx;
  text-align: center;
  font-size: 43rpx;
}
.count-div .top3 .item .l-t .c text,.count-div .top3 .item .d .c text{
  line-height: 60rpx;
  text-align: center;
  font-size: 18rpx;
  margin-left: 3rpx;
}
.count-div .top3 .item .l-t .t{
  color:#fff;

  font-size: 22rpx;
}
.count-div .top3 .item .d{
  
}
.count-div .top3 .item .d .t{
  color:#9C9C9C;
  text-align: center;
  font-size: 22rpx;
}
.count-div .top3 .bottom{
  font-size: 25rpx;
  color: #FFFFFF;
  margin-top: 36rpx;
  margin-bottom: 13rpx;
}
.count-div .top3 .bottom .t{
  margin-bottom: 13rpx;
}
.count-div .top3 .bottom .d{
  display: flex;
  color: #9C9C9C;
}
.count-div .top3 .bottom .d .dian{
  width: 11rpx;
  height: 11rpx;
  background: rgba(255, 0, 32, 1);
  border-radius: 10rem;
  margin: 12rpx 16rpx 12rpx 0;
}

.count-div .top4{
  background: rgba(13, 29, 57, 0.69);
  border-bottom: 1px solid rgba(65, 78, 106, 1);
  margin: 0 31rpx;
  padding: 20rpx 33rpx;
  border-radius: 14rpx;
  color: #fff;
}
.count-div .top4>.t{

  font-size: 25rpx;
  margin-bottom: 16rpx;
}
.count-div .top4 .item{
  background: rgba(255, 255, 255, 0.11);
  padding: 0 0 31rpx 22rpx;
  border-radius: 14rpx;
  margin-bottom: 13rpx;
}
.count-div .top4 .item>.t{
  display: flex;
  justify-content: space-between;
  border-bottom: 1px solid rgba(30, 44, 67, 1);

  font-size: 25rpx;
}

.count-div .top4 .item>.t .l{
  display: flex;
  line-height: 60rpx;
}
.count-div .top4 .item>.t .l text{
  margin-left: 14rpx;
}
.count-div .top4 .item>.t .l .dian{

  width: 18rpx;
  height: 18rpx;
  border-radius: 10rem;
  margin: 24rpx 16rpx 12rpx 0;
}
.count-div .top4 .item.gao .dian{
  background: rgba(255, 0, 32, 1);

}
.count-div .top4 .item.zhong .dian{
  background: rgba(255, 179, 50, 1);

}
.count-div .top4 .item.di .dian{
  background: rgba(116, 116, 116, 1);
}
.count-div .top4 .item.wu{
  padding: 0 0 0 22rpx;
}
.count-div .top4 .item.wu>.t{
  border: none;
}
.count-div .top4 .item.wu>.t .r{
  display: none;
}

.count-div .top4 .item.kong{
  font-size: 25rpx;
  text-align: center;
  padding: 30rpx 20px;
  background: none;
}

.count-div .top4 .item>.t .r{
  background: rgba(216, 216, 216, 0.11);
  width: 87rpx;
  height: 36rpx;
  text-align: center;
  border-radius: 25rpx;
  margin:13rpx;
  line-height: 36rpx;
}
.count-div .top4 .item>.t .r .iconfont{
  font-size: 29rpx;
  transition:All 0.4s ease-in-out;

}

.count-div .top4 .item .d-group{
  transition:All 1s ease;
  overflow: hidden;

}
.count-div .top4 .item.close{
  padding: 0 0 0 22rpx;
}

.count-div .top4 .item.close>.t .r .iconfont{
  transform:rotate(180deg);
}
.count-div .top4 .item.close .d-group{
  height: 0;
}
.count-div .top4 .item.close>.t{
  border: none;

}

.count-div .top4 .item .d-group>.d .t{
  font-size: 25rpx;
  margin-bottom: 13rpx;
  margin-top: 16rpx;
}
.count-div .top4 .item .d-group>.d .dd{
  color: rgba(156, 156, 156, 1);
  display: flex;
  font-size: 22rpx;
  justify-content: space-between;
}
.count-div .top4 .item .d-group>.d .dd .dd-l{
  display: flex;
}
.count-div .top4 .item .d-group>.d .dd .dd-r{
  display: flex;
  margin-right: 20rpx;
}
.count-div .top4 .item .d-group>.d .dd .dian{
  width: 11rpx;
  height: 11rpx;
  border-radius: 10rem;
  margin: 12rpx 16rpx 12rpx 0;
}
.count-div .top5{

  font-size: 25rpx;
  color: #fff;
  background: rgba(19, 36, 66, 1);
  margin: 29rpx 31rpx 4rpx 31rpx;
  padding: 20rpx 33rpx;
  border-radius: 14rpx;

}
.count-div .top5 .btn{
  background: rgba(216, 216, 216, 0.11);
  width: 145rpx;
  height: 43rpx;
  line-height: 43rpx;

  font-size: 25rpx;
border-radius: 25rpx;
text-align: center;
margin: 20rpx auto 10rpx auto;
}

.block{
  margin: 36rpx 31rpx 4rpx 31rpx;
  padding-top: 1rpx;
  border-radius: 14rpx;
  padding-bottom: 6rpx;
}
.block .big-title{
  margin:30rpx 29rpx;
  height: 100rpx;
  line-height: 100rpx;
  border-radius: 14rpx;
  color: #fff;
  display: flex;
  justify-content: space-around;
  font-size: 43rpx;
  font-weight: 800;
  padding-left: 22rpx;
}
.block .big-title text{

  font-size: 29rpx;
}
.block>.item{
  background: #FFF;
  margin: 0 31rpx 27rpx 31rpx;
  padding: 10rpx 18rpx 12rpx 18rpx;
  border-radius: 14rpx;
}
.block>.item>.t{
  padding-left: 24rpx;
  padding-right: 16rpx;
  font-size: 25rpx;
  font-weight: 800;
  margin-top: 27rpx;
  margin-bottom: 20rpx;
  display: flex;
  justify-content: space-between;
}

.block>.item>.t .iconfont{
  background: rgba(216, 216, 216, 1);
  width: 87rpx;
  height: 32rpx;
  text-align: center;
  border-radius: 25rpx;
  line-height: 32rpx;
  margin-top: 5rpx;
  color:rgb(136, 136, 136);
  /* transition:All 0.4s ease-in-out; */
}
.block>.item>.t.open .iconfont{
  transform:rotate(180deg);
}
.block>.item>.c{
  font-weight: 800;
  padding-left: 24rpx;
  font-size: 87rpx;
  margin-top: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items:baseline;
  padding-right: 14rpx;
}
.block>.item>.c text{

  color: #B1B1B1;
  font-size: 25rpx;
  margin-left:10rpx;
}
.block>.item>image{
  width: 100%;
}
.block>.item>.c .r{
  font-size: 25rpx;
  font-weight: 800;
  line-height: 50rpx;
  position: relative;
  padding-left: 38rpx;
}
.block>.item>.c .r image{
  width: 33rpx;
  height: 33rpx;
  position: absolute;  
  left: 0;
  top: 9rpx;
}
.block>.item .table{
  padding-left: 24rpx;
  padding-right: 14rpx;
}
.block>.item .table .th,.block>.item .table .tr{
  display: flex;
  margin-bottom: 18rpx;
}
.block>.item .table .th{
  color: rgba(177, 177, 177, 1);
  font-size: 22rpx;
}
.block>.item .table .th .td:nth-child(1),.block>.item .table .tr .td:nth-child(1){
  width: 28%;
}
.block>.item .table .th .td:nth-child(2),.block>.item .table .tr .td:nth-child(2){
  width: 18%;
}
.block>.item .table .th .td:nth-child(3),.block>.item .table .tr .td:nth-child(3){
  text-align: center;
  width: 9%;
  margin-right: 3%;
}
.block>.item .table .th .td:nth-child(4),.block>.item .table .tr .td:nth-child(4){
  width: 26%;
}
.block>.item .table .th .td:nth-child(5),.block>.item .table .tr .td:nth-child(5){
  width: 16%;
}
.block>.item .table .tr{
  border-bottom: 1px solid rgba(223, 223, 223, 1);
  padding-bottom: 16rpx;
  line-height: 1;

}
.block>.item .table .tr:last-child{
  border: none;
}
.block>.item .table .tr .td{
  font-size: 25rpx;
  font-weight: 800;
}
.block>.item .table .tr .td image{
  width: 14rpx;
  height: 14rpx;
}

.block>.item .table .tr .iconjiantouxia{
  /* transition:All 0.4s ease-in-out; */
  border-radius: 25rpx;
  color: rgb(136, 136, 136);
  text-align: center;
  background: #D8D8D8;
  /* height: 36rpx; */
  /* line-height: 36rpx; */
}
.block>.item>.info{
  padding-left: 24rpx;
  padding-right: 14rpx;
  padding-bottom: 30rpx;
  font-size: 25rpx;
}
.block>.item>.info>.item:nth-child(1){
  margin-top: 17rpx;
}
.block>.item>.info>.item{
  margin-top: 31rpx;
}
.block>.item>.info>.item .t{
  margin-bottom: 5rpx;
  color: #ABABAB;
}


